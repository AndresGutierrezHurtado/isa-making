        "jsonwebtoken": "^9.0.2",
        "next": "15.3.0",
        "next-auth": "^4.24.11",
        "next-test-api-route-handler": "^4.0.16",
        "pg": "^8.16.0",
        "pg-hstore": "^2.3.4",
        "react": "^19.0.0",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^19.0.0",
        "react-icons": "^5.5.0",
        "react-markdown": "^10.1.0",
        "sequelize": "^6.37.7",
        "sequelize-cli": "^6.6.2",
        "swagger-ui-react": "^5.21.0",
        "sweetalert2": "^11.17.2",
        "valibot": "^1.0.0"
    },
