        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^29.5.14",
        "daisyui": "^5.0.19",
        "eslint": "^9",
        "eslint-config-next": "15.3.0",
        "jest": "^30.0.0",
        "jest-environment-jsdom": "^30.0.0",
