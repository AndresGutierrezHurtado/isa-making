{
    "name": "isa-making",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
        "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
        "db:seed:reset": "npm run db:seed:undo && npm run db:seed",
        "test:reset": "npx sequelize-cli db:migrate:undo:all --env test && npx sequelize-cli db:migrate --env test && npx sequelize-cli db:seed:all --env test",
        "test": "jest"
    },
    "dependencies": {
        "@uiw/react-md-editor": "^4.0.5",
