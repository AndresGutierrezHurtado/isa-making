{
    "name": "isa-making",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
        "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
        "db:seed:reset": "npm run db:seed:undo && npm run db:seed"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "mysql2": "^3.14.0",
        "next": "15.3.0",
        "pg-hstore": "^2.3.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-icons": "^5.5.0",
        "sequelize": "^6.37.7",
        "sequelize-cli": "^6.6.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "daisyui": "^5.0.19",
        "eslint": "^9",
        "eslint-config-next": "15.3.0",
        "tailwindcss": "^4"
    }
}
